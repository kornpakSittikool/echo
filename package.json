{
  "name": "echo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "prepare": "husky",
    "lint": "next lint",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,css,md,json,html}\" --write",
    "format:check": "prettier \"**/*.{js,jsx,ts,tsx,css,md,json,html}\" --check",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "up:build": "docker compose up -d --build",
    "up": "docker compose up"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "eslint": "9.39.0",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "typescript-eslint": "^8.46.2",
    "vitest": "^2.1.9"
  }
}
